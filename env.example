# ===== Server (luta-cover-generator/server) =====
PORT=3002

# ===== Volcengine Ark (Doubao) =====
# Get from Ark console: 系统管理 -> API Key 管理
ARK_API_KEY=YOUR_ARK_API_KEY
# Default from Ark examples
ARK_API_BASE=https://ark.cn-beijing.volces.com/api/v3
# Optional: override model id
ARK_TEXT_MODEL=doubao-seed-1-8-251215

# ===== APIMart (current existing provider) =====
APIMART_API_KEY=YOUR_APIMART_KEY
APIMART_API_BASE=https://api.apimart.ai
APIMART_TEXT_MODEL=gemini-3-pro-image-preview
IMAGE_MODEL=gemini-3-pro-image-preview
VISION_MODEL=gemini-2.5-pro

# ===== MiMo-V2-Flash (if you have an OpenAI-compatible endpoint) =====
# If you self-host via vLLM/TGI etc, put your /v1 base here:
MIMO_API_BASE=http://127.0.0.1:8000/v1
MIMO_API_KEY=YOUR_MIMO_KEY_IF_NEEDED
MIMO_MODEL=mimo-v2-flash

# Default model shown/used by the UI for analysis
DEFAULT_TEXT_MODEL_ID=doubao-seed-1-8


